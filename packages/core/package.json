{
  "name": "@zd~/canvas",
  "version": "0.0.6",
  "private": false,
  "description": "A canvas library.",
  "author": "zd",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhuddan/canvas.git"
  },
  "keywords": [
    "canvas"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs.js"
      }
    },
    "./*": "./*"
  },
  "main": "index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "index.js",
    "scripts/*"
  ],
  "scripts": {
    "bumpp": "bumpp",
    "typedoc": "npx typedoc --out ./docs ./src/index.ts"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1"
  }
}
