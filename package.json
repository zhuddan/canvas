{
  "name": "@zd~/canvas",
  "version": "0.0.3",
  "private": false,
  "description": "A canvas library.",
  "author": "zd",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhuddan/canvas.git"
  },
  "keywords": [
    "canvas"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs.js"
      }
    },
    "./*": "./*"
  },
  "main": "index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "index.js",
    "scripts/*"
  ],
  "scripts": {
    "dev:web": "cross-env NODE_ENV=development:web rollup -c rollup.config.dev.mjs -w",
    "dev:wx": "cross-env NODE_ENV=development:wx rollup -c rollup.config.dev.mjs -w",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.build.mjs",
    "publish": "node ./scripts/publish.mjs",
    "release": "bumpp"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.24.1",
    "@dcloudio/types": "^3.4.12",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "bumpp": "^9.4.2",
    "cross-env": "^7.0.3",
    "csstype": "^3.1.3",
    "eslint": "9.5.0",
    "eslint-plugin-format": "^0.1.2",
    "miniprogram-api-typings": "^3.12.3",
    "picocolors": "^1.0.1",
    "rollup": "^4.20.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.6.3",
    "typedoc": "^0.26.6",
    "typedoc-plugin-markdown": "^4.2.5",
    "typescript": "^5.5.4"
  }
}
