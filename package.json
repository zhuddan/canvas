{
  "scripts": {
    "example": "pnpm --filter=example dev",
    "dev": "pnpm dev:web",
    "dev:web": "cross-env NODE_ENV=development:web rollup -c rollup.config.dev.mjs -w",
    "dev:wx": "cross-env NODE_ENV=development:wx rollup -c rollup.config.dev.mjs -w",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.build.mjs",
    "publish": "node ./scripts/publish.mjs",
    "bumpp": "pnpm -F @zd~/canvas bumpp",
    "clean": "rm -rf node_modules pnpm-lock.yaml packages/*/node_modules packages/*/pnpm-lock.yaml packages/*/dist",
    "docs:dev": "pnpm -F docs dev",
    "docs:build": "pnpm -F docs build",
    "docs:preview": "pnpm -F docs preview"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.3",
    "@dcloudio/types": "^3.4.12",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "bumpp": "^9.4.2",
    "cross-env": "^7.0.3",
    "csstype": "^3.1.3",
    "eslint": "9.5.0",
    "eslint-plugin-format": "^0.1.2",
    "miniprogram-api-typings": "^3.12.3",
    "picocolors": "^1.0.1",
    "rollup": "^4.20.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "@eslint-react/eslint-plugin": "^1.5.22",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7"
  }
}